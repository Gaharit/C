 #define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <string.h>
#define MAXLINE 1000
main()
{
	int length, i, c;
	int str[MAXLINE];
	while ( != '\0') {
		gets(str);
		for (i = 0; (c = str[i]) != '\0'; i++);
		length = i;
		if (i > 81)
			for (i = 0; (c = str[i]) != '\0'; i++)
				printf (c);
		printf("\n");
	}
}
